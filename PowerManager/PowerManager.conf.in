autostart = "@PLUGIN_POWERMANAGER_AUTOSTART@"
precondition = ["Platform"]
callsign = "org.rdk.PowerManager"
startuporder = "@PLUGIN_POWERMANAGER_STARTUPORDER@"

configuration = JSON()
rootobject = JSON()

rootobject.add("mode", "@PLUGIN_POWERMANAGER_MODE@")
